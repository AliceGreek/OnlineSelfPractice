<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-action>#{bean.checkStudent}</from-action>
            <from-outcome>Ok</from-outcome>
            <to-view-id>/practice.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{bean.checkStudent}</from-action>
            <from-outcome>NotOk</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/practice.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>mockinglink</from-outcome>
            <to-view-id>/mockingPractice.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mockingPractice.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>java</from-outcome>
            <to-view-id>/java.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/adminPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/adminPage.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{bean.checkAdmin}</from-action>
            <from-outcome>notAdmin</from-outcome>
            <to-view-id>/notAdmin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{bean.checkAdmin}</from-action>
            <from-outcome>yesAdmin</from-outcome>
            <to-view-id>/yesAdmin.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>checkStu</from-outcome>
            <to-view-id>/showStuPerformance.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addQuestions</from-outcome>
            <to-view-id>/addQuestion.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/addQuestion.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{bean.addQuesIntoDatabase()}</from-action>
            <from-outcome>added</from-outcome>
            <to-view-id>/addQuesSuccess.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>modifyQues</from-outcome>
            <to-view-id>/modifyQuesDetails.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>yesAdmin</from-outcome>
            <to-view-id>/yesAdmin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>onlinePractice</from-outcome>
            <to-view-id>/practice.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registerSuccess</from-outcome>
            <to-view-id>/regisSuccess.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>full</from-outcome>
            <to-view-id>/registerFailed.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registrationForm</from-outcome>
            <to-view-id>/registerPage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>endExam</from-outcome>
            <to-view-id>/endExam.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>mockinglink</from-outcome>
            <to-view-id>/mockingPractice.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>scoreChanged</from-outcome>
            <to-view-id>/modifySuccess.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/yesAdmin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>modifyQuestions</from-outcome>
            <to-view-id>/modifyQues.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>addQuestions</from-outcome>
            <to-view-id>/addQuestion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>checkBatch</from-outcome>
            <to-view-id>/showBatchPerformance.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/modifyQuesDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>modified</from-outcome>
            <to-view-id>/modifySuccess.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
</faces-config>
